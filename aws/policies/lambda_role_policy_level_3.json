{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:PutLogEvents"
      ],
      "Resource": "arn:aws:logs:*:*:*"
    },
    {
      "Effect": "Allow",
      "Action": [
        "REPLACE-WITH-A-VALID-ACTION"
      ],
      "Resource": "arn:aws:s3:::REPLACE-WITH-YOUR-BUCKET-NAME/*"
    }
  ]
}
